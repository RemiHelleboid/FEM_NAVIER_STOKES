
@phdthesis{saramito_methodes_2013,
	type = {lecture},
	title = {Méthodes numériques en fluides complexes : théorie et algorithmes},
	shorttitle = {Méthodes numériques en fluides complexes},
	url = {https://cel.archives-ouvertes.fr/cel-00673816},
	abstract = {Les fluides complexes regroupent tous les fluides dont le comportement diffère de celui de l'eau ou l'air : ils ne peuvent pas être décrits par les équations de Navier-Stokes, et sont alors dit non-newtoniens. La plupart des fluides quotidien, tels que les matières plastiques lors de la mise en forme, les fluides géophysiques comme la lave, les sols et roches à grande échelle, ou encore le sang et les tissus biologiques sont des fluides complexes, non-newtoniens. Ce texte traite l'analyse mathématique et numérique des équations décrivant de tels fluides. Les méthodes numériques étudiées ont pour but de calculer les vitesses, pressions et les contraintes pour de tels fluides et pour des géométries suffisamment générales pour qu'un calcul explicite ne soit plus possible. De nombreux exemples illustrent le livre. Ce document est issu des notes d'un cours de deuxième année de maîtrise ainsi que de l'école doctorale de mathématiques appliquées de Grenoble. {\textless}br{\textgreater}{\textless}br{\textgreater} Le cours est disponible en vidéo à l'adresse suivante : http://mi2s.imag.fr/pm/methodes-numeriques-en-fluides-complexes},
	language = {en},
	urldate = {2020-12-02},
	school = {Mathématiques appliquées},
	author = {Saramito, Pierre},
	month = dec,
	year = {2013},
	note = {Pages: 126},
	file = {Full Text PDF:/home/remi/snap/zotero-snap/common/Zotero/storage/HAVCQS52/Saramito - 2013 - Méthodes numériques en fluides complexes  théorie.pdf:application/pdf;Snapshot:/home/remi/snap/zotero-snap/common/Zotero/storage/KQ2EE6QY/cel-00673816.html:text/html}
}

